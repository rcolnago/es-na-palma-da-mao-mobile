<?xml version='1.0' encoding='utf-8'?>
<widget id="br.gov.es.prodest.espm" version="1.2.4" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ES na Palma da M&#xE3;o</name>
    <description>		
        ES na Palma da M&#xE3;o
    </description>
    <author email="presidente@prodest.es.gov.com" href="http://www.prodest.es.gov.br/">		
